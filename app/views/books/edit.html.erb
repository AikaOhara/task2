<h1>Editing Book</h1>

<%= form_for :book, url: book_path, method: :patch do |f| %>
  
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  
  <h4>本文</h4>
  <%= f.text_area :body %>
  
  <br><br>
  <%= f.submit 'Update Book' %>
  <br>
  <%= link_to "Show", book_path(@book.id) %>

<%= link_to "Back", "/books" %>




  
<% end %>